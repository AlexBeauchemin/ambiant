{
  "extends": "airbnb",
  "rules": {
    "arrow-body-style": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "max-len": 0,
    "no-trailing-spaces": 0,
    "react/sort-comp": 0
  },
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "meteor": true
  },
  "globals": {
    "__DEV__": true,
    "Deps": true,
    "gapi": true,
    "Materialize": true,
    "Radios": true,
    "SC": true,
    "ServiceConfiguration": true,
    "WebFont": true,
    "YT": true
  }
}